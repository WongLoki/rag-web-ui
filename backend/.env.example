BACKEND_CORS_ORIGINS= *

# Database settings
MYSQL_SERVER=localhost
MYSQL_USER=postgres
MYSQL_PASSWORD=postgres
MYSQL_DATABASE=ragwebui

# JWT settings
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Vector DB settings
CHROMA_DB_HOST=localhost
CHROMA_DB_PORT=8001

# MinIO settings
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET_NAME=documents

# LLM Service Settings
LLM_SERVICE_TYPE=openai  # openai or ollama

# OpenAI settings
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4

# Ollama settings
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2
OLLAMA_EMBEDDING_MODEL=nomic-embed-text

# Embeddings Service Settings
EMBEDDINGS_SERVICE_TYPE=openai  # openai or ollama

# Vector Store Settings
VECTOR_STORE_TYPE=chroma  # chroma or qdrant
VECTOR_STORE_URL=http://localhost:6333
VECTOR_STORE_PREFER_GRPC=true
